{
  "rewrites": [
    { "source": "/auth/(.*)", "destination": "/api/auth/$1" },
    { "source": "/webhooks/(.*)", "destination": "/api/webhooks/$1" },
    { "source": "/public/(.*)", "destination": "/api/public/$1" },
    { "source": "/admin", "destination": "/api/admin" },
    { "source": "/admin/(.*)", "destination": "/api/admin/$1" },
    { "source": "/assets/(.*)", "destination": "/api/assets/$1" },
    { "source": "/", "destination": "/api" }
  ]
}
